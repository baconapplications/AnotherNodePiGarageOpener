{
  "name": "anotherpinodegarageopener",
  "version": "1.0.0",
  "description": "API and client for our AnotherPiNodeGarageOpener project",
  "main": "server/index.js",
  "scripts": {
    "build-client": "webpack --progress --colors",
    "build-client-prod": "webpack --env=prod --progress --profile --colors",
    "copy:dist": "gulp clean-dist && gulp copy",
    "clean": "echo 'this will display an error on windows - feel free to ignore' && npm run clean:js  && npm run clean:n",
    "clean:js": "gulp clean-js",
    "clean:n": "rimraf ./node_modules",
    "nodemon:w": "nodemon --delay 2000ms server/index.js -e js --watch lib",
    "publish": "gulp clean-js && tsc --sourceMap false && npm run copy:dist",
    "server-client": "webpack-dev-server --inline --progress --port 3434 --content-base client",
    "start": "npm run server-client",
    "start-server": "tsc && concurrently -kr \"npm run tsc:w\" \"npm run nodemon:w\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "typings:install": "typings install"
  },
  "author": "Randy Bacon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/baconapplications/AnotherPiNodeGarageOpener.git"
  },
  "dependencies": {
    "express": "~4.14.0",
    "body-parser": "~1.16.0",
    "compression": "~1.6.2",
    "moment": "~2.17.1",
    "mongodb": "~2.2.22",
    "morgan": "~1.7.0",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@angular/animations": "~4.0.3",
    "@angular/common": "~4.0.3",
    "@angular/compiler": "~4.0.3",
    "@angular/compiler-cli": "~4.0.3",
    "@angular/core": "~4.0.3",
    "@angular/forms": "~4.0.3",
    "@angular/http": "~4.0.3",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "~4.0.3",
    "@angular/platform-browser-dynamic": "~4.0.3",
    "@angular/platform-server": "~4.0.3",
    "@angular/router": "~4.0.3",
    "@angular/upgrade": "~4.0.3",
    "@types/body-parser": "~1.16.3",
    "@types/compression": "~0.0.33",
    "@types/express": "~4.0.35",
    "@types/hammerjs": "^2.0.34",
    "@types/mongodb": "^2.1.43",
    "@types/morgan": "~1.7.32",
    "@types/node": "^7.0.8",
    "@types/winston": "~2.3.0",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "hammerjs": "^2.0.8",
    "ie-shim": "^0.1.0",
    "ng2-cookies": "^1.0.6",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "~5.3.0",
    "sass-loader": "^6.0.3",
    "to-string-loader": "^1.1.4",
    "typescript": "^2.2.2",
    "typings": "^2.1.1",
    "webpack": "~2.4.1",
    "webpack-dev-server": "~2.4.4",
    "webpack-merge": "~4.1.0",
    "zone.js": "0.8.5"
  }
}
